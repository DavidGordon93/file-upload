# microservice specific routes

GET        /envelope/:id                                    @uk.gov.hmrc.fileupload.controllers.EnvelopeController.show(id: uk.gov.hmrc.fileupload.EnvelopeId)
POST       /envelope                                        @uk.gov.hmrc.fileupload.controllers.EnvelopeController.create()
DELETE     /envelope/:id                                    @uk.gov.hmrc.fileupload.controllers.EnvelopeController.delete(id: uk.gov.hmrc.fileupload.EnvelopeId)
PUT        /envelope/:envelopeId/file/:fileId/content       @uk.gov.hmrc.fileupload.controllers.FileController.upsertFile(envelopeId: uk.gov.hmrc.fileupload.EnvelopeId, fileId: uk.gov.hmrc.fileupload.FileId)
GET        /envelope/:envelopeId/file/:fileId/content       @uk.gov.hmrc.fileupload.controllers.FileController.downloadFile(envelopeId: uk.gov.hmrc.fileupload.EnvelopeId, fileId: uk.gov.hmrc.fileupload.FileId)
PUT        /envelope/:envelopeId/file/:fileId/metadata      @uk.gov.hmrc.fileupload.controllers.FileController.metadata(envelopeId: uk.gov.hmrc.fileupload.EnvelopeId, fileId: uk.gov.hmrc.fileupload.FileId)
GET        /envelope/:envelopeId/file/:fileId/metadata      @uk.gov.hmrc.fileupload.controllers.FileController.retrieveMetadata(envelopeId: uk.gov.hmrc.fileupload.EnvelopeId, fileId: uk.gov.hmrc.fileupload.FileId)
